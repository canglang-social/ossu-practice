;; Image,Image -> Boolean
;; Given two images shape like rectangle, return true if First is larger than another

(check-expect (first-img-larger? (ractangle 10 20 "solid" "blue") (ractangle 20 20 "solid" "red")) true)
(check-expect (first-img-larger? (ractangle 10 20 "solid" "blue") (ractangle 10 20 "solid" "red")) false)


(define (first-img-larger? img1 img2) true)